{
  "name": "eslint-config-samsao-base",
  "version": "1.0.2",
  "description": "Samsao's base JS ESLint config",
  "main": "index.js",
  "scripts": {
    "prelint": "editorconfig-tools check * rules/*",
    "lint": "eslint .",
    "prepublish": "(not-in-publish || yarn test) && safe-publish-latest",
    "pretest": "yarn run --silent lint",
    "test": "eslint-find-rules --unused && babel-tape-runner ./test/test-*.js",
    "travis": "yarn run --silent test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samsao/javascript"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "samsao",
    "javascript",
    "styleguide"
  ],
  "author": "Samsao Development Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samsao/javascript/issues"
  },
  "homepage": "https://github.com/samsao/javascript",
  "peerDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-import": "^2.2.0"
  },
  "devDependencies": {
    "babel-preset-airbnb": "^2.2.3",
    "babel-tape-runner": "^2.0.1",
    "editorconfig-tools": "^0.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-find-rules": "^1.14.3",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "in-publish": "^2.0.0",
    "safe-publish-latest": "^1.1.1",
    "tape": "^4.6.3"
  },
  "engines": {
    "node": ">= 4"
  }
}
